/* pages/mineWish/mineWish.wxss */
.mine-wish {
  position: relative;
}
.mine-wish view {
  position: relative;
  padding: 20rpx;
  border-bottom: 2rpx solid rgb(230,230,230);
}
.mine-wish view text:nth-child(1) {
  display: block;
  margin-bottom: 20rpx;
}
.mine-wish view text:nth-child(2) {
  color: #666;
  font-size: 24rpx;
}
.mine-wish view text:nth-child(3) {
  position: absolute;
  right: 20rpx;
  top: 20rpx;
  padding: 10rpx 20rpx;
  font-size: 24rpx;
  background: rgb(230,230,230);
  border-radius: 30rpx;
}
.integral-bottom {
  position: absolute;
  left: 0;
  top: 300rpx;
  right: 0;
  bottom: 0;
  margin: auto;
  width: 220rpx;
  height: 200rpx;
  text-align: center;
}
.integral-bottom image {
  width: 100%;
  height: 100%;
}
.fabu {
  position: absolute;
  right: 20rpx;
  top: 900rpx;
  width: 120rpx;
  height: 120rpx;
}
